    "@types/offscreencanvas": "^2019.7.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vladmandic/build": "^0.9.3",
    "@vladmandic/pilogger": "^0.4.9",
    "@vladmandic/tfjs": "github:vladmandic/tfjs",
