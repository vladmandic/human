  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladmandic/human.git"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@tensorflow/tfjs": "^2.7.0",
    "@tensorflow/tfjs-backend-wasm": "^2.7.0",
    "@tensorflow/tfjs-node": "^2.7.0",
    "@vladmandic/pilogger": "^0.2.7",
    "chokidar": "^3.4.3",
    "dayjs": "^1.9.6",
    "esbuild": "^0.8.8",
    "eslint": "^7.13.0",
